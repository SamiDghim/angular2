{
  "name": "angular2",
  "version": "1.0.0",
  "scripts": {
    "build": "rm -rf app && tsc -p src",
    "postinstall": "npm run typings install",
    "lite": "lite-server",
    "gulp": "gulp",
    "start": "concurrent \"npm run gulp\" \"npm run lite\" ",
    "typings": "typings",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "live": "live-server",
    "test": "live-server --open=unit-tests.html",
    "test:w": "concurrent  \"npm run tsc:w\" \"npm run test\" ",
    "coverage": "http-server -c-1 -o -p 9875 ./coverage"
  },
  "license": "ISC",
  "dependencies": {
    "angular2": "2.0.0-beta.9",
    "@angular2-material/button": "^2.0.0-alpha.3",
    "@angular2-material/core": "^2.0.0-alpha.3",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "ng2-material": "^0.3.6",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "0.19.24"
  },
  "devDependencies": {
    "autoprefixer": "^6.2.1",
    "concurrently": "1.0.0",
    "cssnano": "^3.4.0",
    "gulp": "^3.9.0",
    "gulp-ext-replace": "^0.2.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-postcss": "^6.0.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-typescript": "^2.10.0",
    "del": "^2.1.0",
    "gulp-uglify": "^1.5.1",
    "http-server": "^0.8.5",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-coverage": "^0.5.5",
    "karma-jasmine": "^0.3.8",
    "lite-server": "^2.0.1",
    "live-server": "1.0.0",
    "postcss": "^5.0.13",
    "postcss-scss": "^0.1.3",
    "precss": "^1.3.0",
    "remap-istanbul": "^0.6.3",
    "systemjs": "^0.19.24",
    "tsd": "^0.6.5-beta",
    "typescript": "^1.8.10",
    "typings": "^0.6.8"
  }
}
